# Staging Environment Configuration
NODE_ENV=staging
PORT=3002

# Database URLs (Staging)
# Add your staging database connection strings here
# DATABASE_URL=

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6381
REDIS_PASSWORD=

# Firebase Configuration (Staging)
# FIREBASE_PROJECT_ID=tulink-staging
# FIREBASE_CLIENT_EMAIL=
# FIREBASE_PRIVATE_KEY=
# FIREBASE_DATABASE_URL=

# Maps & Location Services
# GOOGLE_MAPS_API_KEY=
# MAPBOX_ACCESS_TOKEN=

# JWT Configuration
# JWT_SECRET=your-staging-jwt-secret-key
# JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=https://staging.tulink.xyz,https://api.staging.tulink.xyz

# API Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# Swagger Documentation
SWAGGER_ENABLED=true